{
    "action": {
        "default_icon": {
            "16": "img/logo16.png",
            "32": "img/logo32.png",
            "48": "img/logo48.png",
            "64": "img/logo64.png",
            "128": "img/logo128.png"
        },
        "default_popup": "popup.html"
    },
    "author": "Vavien. & Dekamir",
    "background": {
        "service_worker": "js/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://normalsozluk.com/*"
            ],
            "js": [
                "js/injectCSS.js",
                "js/dom.js",
                "js/contentScript.js"
            ],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://normalsozluk.com/*"
            ],
            "js": [
                "js/jqueryEdits.js"
            ],
            "run_at": "document_idle",
            "world": "MAIN"
        }
    ],
    "description": "normal sözlük'ü sıkar atar (web sitesini iyileştirir).",
    "homepage_url": "https://github.com/Dekamir",
    "host_permissions": [ "<all_urls>" ],
    "icons": {
        "16": "img/logo16.png",
        "32": "img/logo32.png",
        "48": "img/logo48.png",
        "64": "img/logo64.png",
        "128": "img/logo128.png"
    },
    "manifest_version": 3,
    "name": "sıkılmış portakal",
    "permissions": [
        "scripting",
        "storage",
        "tabs"
    ],
    "version": "1.0.0",
    "version_name": "1"
}
